{
  "name": "crapjack",
  "version": "0.0.4",
  "private": false,
  "description": "CrapJack Game",
  "homepage": "https://crapjack.herokuapp.com",
  "repository": "git@github.com:maxim-andrews/crapjack.git",
  "author": "Maxim Andrews <maximandrewz@users.noreply.github.com> (http://maximandrews.com)",
  "license": "MIT",
  "scripts": {
    "start": "fullstack-scripts start",
    "build": "fullstack-scripts build",
    "test": "fullstack-scripts test --env=jsdom",
    "heroku-postbuild": "fullstack-scripts build"
  },
  "devDependencies": {
    "bootstrap": "^4.1.3",
    "connected-react-router": "^4.4.1",
    "fullstack-scripts": "0.0.7",
    "node-sass": "^4.9.3",
    "postcss-preset-env": "^5.3.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0"
  },
  "dependencies": {
    "koa": "^2.5.2",
    "koa-compress": "^3.0.0",
    "koa-rewrite": "^3.0.1",
    "koa-static": "^4.0.3"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 9"
    ]
  },
  "serviceWorkerPreCache": {
    "navigateFallback": "/index.html",
    "navigateFallbackWhitelist": [
      {
        "regexp": "^(?!\\/__).*"
      }
    ],
    "staticFileGlobsIgnorePatterns": [
      {
        "regexp": "\\.map$"
      },
      {
        "regexp": "asset-manifest\\.json$"
      }
    ]
  },
  "engines": {
    "node": "v8.11.3",
    "npm": "6.4.0"
  }
}
